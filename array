void main(){
    int a[]={7,8,9,11,10};
    int l=0,s=0,i;
    int n=sizeof(a)/sizeof(a[0]);
    for (i=0;i<n;i++){
        if (a[i]>l){
            s=l;
            l=a[i];
        }
            else if(a[i]>s){
            
                s=a[i];
            }
        }
        printf("the second largest is%d",s);
    }
